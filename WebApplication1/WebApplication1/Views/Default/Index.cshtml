
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div style="background-color:#91bde6" > 

        @DateTime.Now.ToShortDateString()
    </div>
    <br />
    <div style="background-color:#ffd800">

        @for (int i=0;i<=10;i++)
        {
            @i
            <br />
        }

    </div>
    <br />
   
    <div style="background-color:#91bde6">
       @{ 
           List<string> deneme = new List<string>() { "serkan", "altunay", "deneme" };

           foreach (var item in deneme)
           {
               @item
               <br />

           }
       }
    </div>
</body>
</html>
